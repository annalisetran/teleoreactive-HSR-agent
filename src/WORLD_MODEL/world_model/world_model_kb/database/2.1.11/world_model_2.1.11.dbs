		</view>
		<view name="v_scene" >
			<view_script><![CDATA[CREATE OR REPLACE VIEW ${nameWithSchemaName} AS SELECT so.scene_id, s.scene_ts, so.object_id, oc.name
FROM public.scene_object so 
inner join public.scene s on so.scene_id = s.scene_id
inner join public.object o on so.object_id = o.object_id
left outer join public.object_class oc on o.object_class_id = oc.object_class_id]]></view_script>
